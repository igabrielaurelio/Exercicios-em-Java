import java.util.Scanner;


public class Main {

	public static void main (String[] args){
	
	Scanner sc= new Scanner(System.in);
	int numero;
	
	System.out.println("Digite um numero");
	numero = sc.nextInt();//Escaneia o numero digitado pelo usuário
	
	if(numero>0){
		System.out.println("Positivo");//Caso o numero seja positivo ele vai imprimir que é positivo
	} else if(numero<0){
		System.out.println("Negativo");//Caso contrario, negativo
	} else{
		System.out.println("Neutro");
		
		}
	sc.close();
	}
}
