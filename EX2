import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // Criar um objeto Scanner para receber entrada do usuário
        Scanner sc = new Scanner(System.in);
        
        // Declaração da variável para armazenar o número digitado pelo usuário
        int numero;
        
        // Solicitar que o usuário digite um número
        System.out.println("Digite um número");
        
        // Ler o número fornecido pelo usuário
        numero = sc.nextInt();
        
        // Verificar se o número é par ou ímpar e imprimir o resultado correspondente
        if (numero % 2 == 0) {
            System.out.println("Par");
        } else {
            System.out.println("Ímpar");
        }
        
        // Fechar o objeto Scanner para liberar recursos
        sc.close();
    }
}
